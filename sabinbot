import asyncio
import discord

client = discord.Client()

token = "token"

@client.event
async def on_ready():
    print("다음으로 로그인합니다 : ")
    print(client.user.name)
    print(client.user.id)
    print("===========")
    game = discord.Game("사비니")
    await client.change_presence(status=discord.Status.online, activity=game)

@client.event
async def on_message(message):
    if message.author.bot:
        return None
    if message.content == ("안녕"):
        await message.channel.send("안녕!")
    if message.content == ("S명령어"):
        embed = discord.Embed(title="이거슨 제목이라 합니다!", description="이거슨 설명이라고 합니다!", color=0x00ff00)
        embed.set_footer(text="이거슨 푸터라고 합니다!")
        embed.set_image(url="https://i.imgur.com/xzPCXp8.jpg")
        await message.channel.send(message.channel, embed=embed)

client.run(token)
